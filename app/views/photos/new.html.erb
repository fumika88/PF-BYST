<div class="container-fulid">
<div class="row">
<div class="col-md-12">

<h1>画像投稿フォーム</h1>
<%= form_with model: @photo, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>名前</h4>
  <%= f.text_field :name %>
  <h4>説明</h4>
  <%= f.text_area :caption %>
  <%= f.submit '投稿' %>
<% end %>

</div>
</div>
</div>