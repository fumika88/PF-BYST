<div class="container-fulid">
  <h2>ハッシュタグ #<%= @tag.hashname %></h2>
<div class="row">

<% @photos.each do |photo| %>
<div class="col-md-2 mt-3 mx-5">
<div>
<%= link_to photo_path(photo.id) do %>
<%= attachment_image_tag photo, :image, :fill, 300, 300, format: 'jpeg', fallback: 'noimage.jpg', size: '300x300', class: "rounded" %>
<% end %>
</div>
</div>
<% end %>

</div>
</div>
