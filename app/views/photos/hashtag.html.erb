<div class="container">
  <h3 class="text-center my-3">HashTag Lists</h3>
  <h3 class="text-center my-3">[ #<%= @tag.hashname %> ]</h3>
<div class="row">


<% @photos.each do |photo| %>
<div class="col-md-3.5 mt-2 mx-auto my-3">
<div>
<%= link_to photo_path(photo.id) do %>
<%= attachment_image_tag photo, :image, :fill, 300, 300, format: 'jpeg', fallback: 'noimage.jpg', size: '300x300', class: "rounded" %>
<% end %>
</div>
</div>
<% end %>

</div>
</div>
